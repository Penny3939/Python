   "source": [
    "num = input().split() #2 3 \n",
    "num1 = int(num[0])\n",
    "num2 = int(num[1])\n",
    "\n",
    "times_1 = 0\n",
    "\n",
    "while times_1 < num1:\n",
    "  times_2 = 0\n",
    "  while times_2 < num2:\n",
    "    print(f\"{times_1+1}*{times_2 +1} = {(times_1+1)*(times_2 +1)}\")\n",
    "    \n",
    "    times_2 += 1\n",
    "\n",
    "  times_1 += 1"
   ]
